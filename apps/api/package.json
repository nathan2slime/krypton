{
  "name": "@kry/api",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "start": "node dist/main",
    "dev": "nest start --watch",
    "debug": "nest start --debug --watch",
    "prod": "node dist/main",
    "test": "jest",
    "seed": "nestjs-command seed:run",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "e2e": "jest --config ./tests/jest-e2e.json"
  },
  "dependencies": {
    "@kry/envs": "*",
    "@kry/logger": "*",
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@apollo/server": "^4.7.0",
    "@apollo/subgraph": "^2.4.3",
    "@faker-js/faker": "^8.0.1",
    "@nestjs/apollo": "^11.0.5",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/graphql": "^11.0.5",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@nestjs/typeorm": "^9.0.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/graphql-fields": "^1.3.5",
    "@types/jest": "29.5.0",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "18.15.11",
    "@types/nodemailer": "^6.4.8",
    "@types/supertest": "^2.0.11",
    "axios": "^1.4.0",
    "babel-jest": "^29.5.0",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.14.0",
    "date-fns": "^2.30.0",
    "graphql": "^16.6.0",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.21.3",
    "jest": "29.5.0",
    "jsonwebtoken": "^9.0.0",
    "mysql2": "^3.2.4",
    "nestjs-command": "^3.1.3",
    "nestjs-typeorm-paginate": "^4.0.3",
    "nodemailer": "^6.9.3",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "tslib": "^2.3.0",
    "type-graphql": "2.0.0-beta.1",
    "typegraphql-nestjs": "^0.5.0",
    "typeorm": "^0.3.15",
    "typescript": "latest"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "tests",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
